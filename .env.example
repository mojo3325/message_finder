### Telegram MTProto (required for listener)
TELEGRAM_API_ID=
TELEGRAM_API_HASH=
TELEGRAM_STRING_SESSION=
TELEGRAM_SESSION_PATH=session

### Telegram Bot API (optional, used for notifications)
# Prefer TELEGRAM_BOT_TOKEN (used by tg/bot_api.py), or set MESSAGE_FINDER_BOT_TOKEN.
TELEGRAM_BOT_TOKEN=
MESSAGE_FINDER_BOT_TOKEN=

### Warp chat bot (required for warp_chat.py if used)
WARP_CHAT_BOT_TOKEN=
WARP_BOT_USERNAME=warp_chat_bot

### LLM providers
ENABLE_GEMINI=true
PROVIDER_ORDER=gemini,cerebras,local

GEMINI_BASE_URL=https://generativelanguage.googleapis.com/v1beta/openai/
GEMINI_API_KEY=
GEMINI_MODEL=gemma-3-27b-it
GEMINI_REPLY_MODEL=gemini-2.5-flash

CEREBRAS_BASE_URL=https://api.cerebras.ai/v1
CEREBRAS_API_KEY=
CEREBRAS_EXTRA_API_KEY=
CEREBRAS_MODEL=gpt-oss-120b

GROQ_BASE_URL=https://api.groq.com/openai/v1
GROQ_API_KEY=
GROQ_REPLY_MODEL=openai/gpt-oss-120b

LMSTUDIO_BASE_URL=http://127.0.0.1:1234
LMSTUDIO_API_KEY=not-needed
LMSTUDIO_MODEL=openai/gpt-oss-20b

### Storage (Supabase; optional)
SUPABASE_URL=
SUPABASE_ANON_KEY=

### Runtime
ACCOUNTS_FILE=data/accounts.json
LOG_LEVEL=INFO
REQUEST_TIMEOUT_S=20
RETRY_MAX_ATTEMPTS=3
